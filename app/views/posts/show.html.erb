<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @post.description %>
</p>
<p><strong>Tags:</strong></p>
<ul>
  <% @post.tags.each do |tag| %>
    <li><%= tag.title %></li>
  <% end %>
</ul>
<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
